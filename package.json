    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.27.0",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.7.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react": "^6.24.1"
  },
