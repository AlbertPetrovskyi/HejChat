{
  "name": "amogus-website",
  "version": "1.0.0",
  "description": "A professional website for the Amogus project, featuring animations and a backend.",
  "main": "client/src/index.jsx",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "client": "cd client && npm run dev",
